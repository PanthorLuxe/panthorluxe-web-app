{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Frame-Options": "SAMEORIGIN",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  }
}